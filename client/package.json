    "@angular/platform-browser-dynamic": "14.0.0",
    "@angular/router": "14.0.0",
    "rxjs": "7.5.0",
    "tslib": "2.3.0",
    "zone.js": "0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.0.5",
    "@angular/cli": "~14.0.5",
    "@angular/compiler-cli": "^14.0.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@types/jest": "^28.1.4",
    "autoprefixer": "^10.4.7",
    "jest": "^28.1.2",
    "jest-preset-angular": "^12.1.0",
